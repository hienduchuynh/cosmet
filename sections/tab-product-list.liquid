{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }    
  }
  @media screen and (min-width: 1200px) {
    .section-{{ section.id }} .section-collapsible{
      width: {{ section.settings.box_width }}%;
      max-width: {{ section.settings.box_width }}%;
      margin: auto;
    }
  }
{%- endstyle -%}
<section class="page-width section-{{ section.id }} section-{{ section.id }}-padding">
  <div class="section-collapsible">
      {{ 'component-tabs.css' | asset_url | stylesheet_tag }}
      <div class="product___tabs product_tabs_positions_{{ section.settings.collapsible_tab_header_position }} tab quick-add-hidden" id="TabDetails-{{ section.id }}" {{ block.shopify_attributes }}>
        <div id="tabsheader" class="tab-header">
          <ul class="nav nav-tabs tab_nav_with_products">
            {%- for block in section.blocks -%}
              {%- case block.type -%}
                {%- when '@app' -%}
                  {% render block %}
                {%- when 'collapsible_tab' -%}
                  {%- if block.settings.content != blank or block.settings.page.content != blank or block.settings.description == true -%}
                    <li id="tabHeader-{{ forloop.index }}">
                      <div class="summary__title">
                        {% render 'icon-accordion', icon: block.settings.icon %}
                        <h2 class="h2 tab__title">
                          {{ block.settings.heading | default: block.settings.page.title }}
                        </h2>
                      </div>
                    </li>
                  {%- endif -%}
                {%- when 'product_tab' -%}
                  <li id="tabHeader-{{ forloop.index }}">
                    <div class="summary__title">
                      {% render 'icon-accordion', icon: block.settings.icon %}
                      <h2 class="h2 tab__title">
                        {{ block.settings.heading | default: block.settings.page.title }}
                      </h2>
                    </div>
                  </li>
              {%- endcase -%}
            {%- endfor -%}
          </ul>
        </div>
        <div id="tabscontent" class="tab-content">
          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when '@app' -%}
                {% render block %}
              {%- when 'collapsible_tab' -%}
                {%- if block.settings.content != blank or block.settings.page.content != blank or block.settings.description == true -%}
                  <div id="TabContent-{{ forloop.index }}" class="tab__content fade rte" {% if forloop.first == true %}style="display:block;"{%- endif -%}>
                    {% if block.settings.description == true %}
                      {{ product.description }}
                    {% else %}
                      {{ block.settings.content }}
                    {%- endif -%}
                    {%- if block.settings.image != blank -%}
                      <div class="media media--{{ block.settings.image_ratio }} media--transparent {% if block.settings.image == blank %} placeholder{% endif %}">
                        {%-liquid
                            assign image_height = block.settings.image.width | divided_by: block.settings.image.aspect_ratio
                            assign image_class = ""
                            assign sizes = "100%"
                          -%}
                        {{ block.settings.image | image_url: width: 1500 | image_tag:
                        loading: 'lazy'
                        , width: block.settings.image.width
                        , height: image_height
                        , class: image_class
                        , sizes: sizes
                        , widths: '375, 550, 750, 1100, 1500, 1780, 2000, 3000, 3840'
                        , alt: block.settings.image.alt | escape
                        }}
                      </div>
                    {%- endif -%}
                    {{ block.settings.page.content }}
                  </div>
                {%- endif -%}
              {%- when 'product_tab' -%}
                <link rel="stylesheet" href="{{ 'template-collection.css' | asset_url }}" media="print" onload="this.media='all'">
                <noscript>{{ 'component-slider.css' | asset_url | stylesheet_tag }}</noscript>
                <noscript>{{ 'template-collection.css' | asset_url | stylesheet_tag }}</noscript>
                {%- if block.settings.enable_quick_add -%}
                  <link rel="stylesheet" href="{{ 'quick-add.css' | asset_url }}" media="print" onload="this.media='all'">
                  <script src="{{ 'quick-add.js' | asset_url }}" defer="defer"></script>
                  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
                {%- endif -%}
                {%- liquid
                  assign products_to_display = block.settings.collection.all_products_count

                  if block.settings.collection.all_products_count > block.settings.products_to_show
                    assign products_to_display = block.settings.products_to_show
                    assign more_in_collection = true
                  endif

                  assign columns_mobile_int = block.settings.columns_mobile | plus: 0
                  assign show_mobile_slider = false
                  if block.settings.swipe_on_mobile and products_to_display > columns_mobile_int
                    assign show_mobile_slider = true
                  endif

                  assign show_desktop_slider = false
                  if block.settings.enable_desktop_slider and products_to_display > block.settings.columns_desktop
                    assign show_desktop_slider = true
                  endif

                  if block.settings.description != blank or block.settings.show_description and block.settings.collection.description != empty 
                    if block.settings.show_description
                      assign collection_description = block.settings.collection.description
                    else
                      assign collection_description = block.settings.description
                    endif
                  endif
                -%}
                <div id="TabContent-{{ forloop.index }}" class="tab__content tab_content_with_products div fade rte" {% if forloop.first == true %}style="display:block;"{%- endif -%}>
                  <slider-component class="slider-mobile-gutter{% if block.settings.full_width %} slider-component-full-width{% endif %}{% if show_desktop_slider == false and block.settings.full_width == false %} page-width-desktop page-width-tablet {% endif %}{% if show_desktop_slider %} slider-component-desktop{% endif %} {% if show_mobile_slider %} slider-component-mobile{% endif %}">
                    {%-liquid
                      assign columns_desktop = block.settings.columns_desktop
                      if products_to_display <= block.settings.columns_desktop
                        assign num_dot_desktop = 1
                      else
                        assign num_dot_desktop = products_to_display | minus: block.settings.columns_desktop | plus: 1
                      endif
              
                      assign columns_mobile = block.settings.columns_mobile
                    -%}
                    {% if block.settings.show_card_collection %}
                      <div class="product-grid-wrap product-grid-wrap--{{ block.settings.desktop_layout }}">
                        <div class="collection-item">
                          {% render 'card-collection'
                          , card_collection: block.settings.collection
                          , media_aspect_ratio: 'portrait'
                          , columns: 1
                          , extend_height: true
                          , description: collection_description
                          %}
                        </div>
                        <div class="product-items">
                        {% endif %}
                        <ul id="Slider-{{ section.id }}" class="grid product-grid contains-card{% if settings.card_style == 'standard' %} contains-card--standard{% endif %} grid--{{ columns_desktop }}-col-desktop{% if block.settings.collection == blank %} grid--2-col-tablet-down{% else %} grid--{{ columns_mobile }}-col-tablet-down{% endif %}{% if show_mobile_slider or show_desktop_slider %} slider{% if show_desktop_slider %} slider--desktop{% endif %}{% if show_mobile_slider %} slider--tablet grid--peek{% endif %}{% endif %}" role="list" aria-label="{{ 'sections.featured_collection.slider' | t }}">
                          {%- for product in block.settings.collection.products limit: block.settings.products_to_show -%}
                            <li id="Slide-{{ section.id }}-{{ forloop.index }}" class="grid__item{% if show_mobile_slider or show_desktop_slider %} slider__slide{% endif %}">
                              {% render 'card-product'
                              , card_product: product
                              , media_aspect_ratio: block.settings.image_ratio
                              , show_secondary_image: block.settings.show_secondary_image
                              , show_vendor: block.settings.show_vendor
                              , show_rating: block.settings.show_rating
                              , show_quick_add: block.settings.enable_quick_add
                              , section_id: section.id
                              , show_product_description: block.settings.show_product_description
                              , show_quick_view: false
                              %}
                            </li>
                          {%- else -%}
                            {%- for i in (1..columns_desktop) -%}
                              <li class="grid__item">
                                {% render 'card-product', show_vendor: block.settings.show_vendor %}
                              </li>
                            {%- endfor -%}
                          {%- endfor -%}
                        </ul>
                        {% if block.settings.show_card_collection %}
                        </div>
                      </div>
                    {%- endif -%}
                    {%- if show_mobile_slider or show_desktop_slider -%}
                      <div class="slider-buttons no-js-hidden slider-{%- if block.settings.slider_visual == 'dots' -%}dots{%- elsif block.settings.slider_visual == 'counter' -%}counter{%- else -%}navigation{%- endif -%}">
                        <button type="button" class="slider-button slider-button--prev" name="previous" aria-label="{{ 'general.slider.previous_slide' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-chevron-left' %}</button>
                        {%- if block.settings.slider_visual != 'nav' -%}
                        <div class="slider-counter caption slider-counter--{{ block.settings.slider_visual }}{% if block.settings.slider_visual == 'counter' or block.settings.slider_visual == 'numbers' %} caption{% endif %}">
                          {%- if block.settings.slider_visual == 'counter' -%}
                            <span class="slider-counter--current">1</span>
                            <span aria-hidden="true">
                              /
                            </span>
                            <span class="visually-hidden">{{ 'general.slider.of' | t }}</span>
                            <span class="slider-counter--total">{{ products_to_display }}</span>
                          {%- else-%}
                            <div class="slideshow__control-wrapper">
                              {%- for block_ in (1..num_dot_desktop) -%}
                                <button class="slider-counter__link slider-counter__link--{{ block.settings.slider_visual }} link" aria-label="{{ 'sections.slideshow.load_slide' | t }} {{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}" aria-controls="Slider-{{ section.id }}">
                                  {%- if block.settings.slider_visual == 'numbers' -%}
                                    {{ forloop.index }}
                                  {% else %}
                                    <span class="dot"></span>
                                  {%- endif -%}
                                </button>
                              {%- endfor -%}
                            </div>
                          {%- endif -%}
                        </div>
                        {%- endif -%}
                        <button type="button" class="slider-button slider-button--next" name="next" aria-label="{{ 'general.slider.next_slide' | t }}" aria-controls="Slider-{{ section.id }}">{% render 'icon-chevron-left' %}</button>
                      </div>
                    {%- endif -%}
                  </slider-component>
              
                  {%- if block.settings.show_view_all and more_in_collection and block.settings.view_all_style != 'link' -%}
                    <div class="center collection__view-all">
                      <a href="{{ block.settings.collection.url }}" class="{% if block.settings.view_all_style == 'link' %}link hightlight-link link--text{% elsif block.settings.view_all_style == 'solid' %}button{% else %}button button--primary{% endif %}" aria-label="{{ 'sections.featured_collection.view_all_label' | t: collection_name: block.settings.collection.title }}">
                        <span>View all products</span>
                      </a>
                    </div>
                  {%- endif -%}
                </div>
            {%- endcase -%}
          {%- endfor -%}
        </div>
      </div>
      <script>
        window.addEventListener("load", function() {
          var container = document.getElementById("TabDetails-{{ section.id }}");
          var tabcon = document.getElementById("tabscontent");
          var navitem = document.getElementById("tabHeader-1");
          var ident = navitem.id.split("-")[1];
          navitem.parentNode.setAttribute("data-current", ident);
          navitem.setAttribute("class", "active");
          var pages = tabcon.getElementsByTagName("div")[0];
          for (var i = 1; i < pages.length; i++) {
            pages.item(i).style.display = "none";
          };
          var tabs = container.getElementsByTagName("li");
          for (var i = 0; i < tabs.length; i++) {
            tabs[i].onclick = displayPage;
          }
          function displayPage() {
            var current = this.parentNode.getAttribute("data-current");
            document.getElementById("tabHeader-" + current).removeAttribute("class");
            document.getElementById("TabContent-" + current).style.display = "none";
            var ident = this.id.split("-")[1];
            this.setAttribute("class", "active");
            document.getElementById("TabContent-" + ident).style.display = "block";
            this.parentNode.setAttribute("data-current", ident);
          }
        });
      </script>
    </div>
</section>
